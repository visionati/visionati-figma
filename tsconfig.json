{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6"],
    "strict": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@figma"
    ],
    "outDir": ".",
    "rootDir": ".",
    "module": "none",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "resolveJsonModule": false,
    "declaration": false,
    "sourceMap": false
  },
  "include": ["code.ts"],
  "exclude": ["node_modules", "code.js"]
}